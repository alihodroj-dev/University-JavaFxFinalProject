+package com.example.javafxfinalproject.Components;
+
+import javafx.scene.control.Button;
+import javafx.scene.image.ImageView;
+import javafx.scene.input.MouseEvent;
+import javafx.scene.layout.HBox;
+import javafx.scene.layout.StackPane;
+
+import java.util.ArrayList;
+
+public class Carousel extends StackPane {
+
+    private final ArrayList<ImageView> images = new ArrayList<>();
+    private int currentIndex = 0;
+    private double dragStartX;
+
+    public Carousel(ArrayList<ImageView> images) {
+        this.images.addAll(images);
+        if (!images.isEmpty()) {
+            getChildren().add(images.getFirst());
+        }
+
+        // Navigation Buttons
+        Button prevButton = new Button("<");
+        Button nextButton = new Button(">");
+
+        prevButton.setOnAction(e -> showPrevious());
+        nextButton.setOnAction(e -> showNext());
+
+        // Button Layout
+        HBox buttonBox = new HBox(10, prevButton, nextButton);
+        buttonBox.setStyle("-fx-alignment: center;");
+        buttonBox.setTranslateY(200); // Adjust positioning
+
+        // Drag Event Handling
+        setOnMousePressed(this::onMousePressed);
+        setOnMouseDragged(this::onMouseDragged);
+        setOnMouseReleased(this::onMouseReleased);
+
+        getChildren().add(buttonBox);
+        setStyle("-fx-background-color: #222; -fx-alignment: center;");
+    }
+
+    private void onMousePressed(MouseEvent event) {
+        dragStartX = event.getX();
+    }
+
+    private void onMouseDragged(MouseEvent event) {
+        double dragDelta = event.getX() - dragStartX;
+        if (dragDelta > 50) { // Swipe right
+            showPrevious();
+            dragStartX = event.getX(); // Reset drag start
+        } else if (dragDelta < -50) { // Swipe left
+            showNext();
+            dragStartX = event.getX(); // Reset drag start
+        }
+    }
+
+    private void onMouseReleased(MouseEvent event) {
+        // Optionally handle releasing logic here
+    }
+
+    private void showPrevious() {
+        if (currentIndex > 0) {
+            getChildren().remove(images.get(currentIndex));
+            currentIndex--;
+            getChildren().add(0, images.get(currentIndex));
+        }
+    }
+
+    private void showNext() {
+        if (currentIndex < images.size() - 1) {
+            getChildren().remove(images.get(currentIndex));
+            currentIndex++;
+            getChildren().add(0, images.get(currentIndex));
+        }
+    }
+
+}
