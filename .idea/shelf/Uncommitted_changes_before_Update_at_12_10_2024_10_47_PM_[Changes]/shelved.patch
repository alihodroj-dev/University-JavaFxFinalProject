+        dragStartX = event.getX();
+    }
+
+    private void onMouseDragged(MouseEvent event) {
+        double dragDelta = event.getX() - dragStartX;
+        if (dragDelta > 50) { // Swipe right
+            showPrevious();
+            dragStartX = event.getX(); // Reset drag start
+        } else if (dragDelta < -50) { // Swipe left
+            showNext();
+            dragStartX = event.getX(); // Reset drag start
+        }
+    }
+
+    private void onMouseReleased(MouseEvent event) {
+        // Optionally handle releasing logic here
+    }
+
+    private void showPrevious() {
+        if (currentIndex > 0) {
+            getChildren().remove(images.get(currentIndex));
+            currentIndex--;
+            getChildren().add(0, images.get(currentIndex));
+        }
+    }
+
+    private void showNext() {
+        if (currentIndex < images.size() - 1) {
+            getChildren().remove(images.get(currentIndex));
+            currentIndex++;
+            getChildren().add(0, images.get(currentIndex));
+        }
+    }
+
+}
